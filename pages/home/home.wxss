    z-index:0;
    visibility:hidden;
    background:rgba(250, 250, 250, .95);
   
}
.welcome.show{
    z-index:97;
    visibility:inherit;
    animation:sliderUp .4s ease-in 2s backwards;
}
.welcome swiper{
    width:100%;
