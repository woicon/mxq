page,input{
    font-family:PingFang SC,Roboto,sans-serif;
    color:#191919;
}
page{
    overflow-x:hidden;
}
@font-face {
	font-family: 'fontico';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: normal;
	font-style: normal;
}
[class^="ico-"], [class*=" ico-"] {
  font-family: 'fontico' !important;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
}

.ico-like-sel:before {
  content: "\e900";
}
.ico-add:before {
  content: "\e901";
}
.ico-like:before {
  content: "\e902";
}
.ico-lock:before {
  content: "\e903";
}
.ico-min:before {
  content: "\e904";
}
.ico-ok:before {
  content: "\e905";
}
.ico-phone:before {
  content: "\e906";
}
.ico-search:before {
  content: "\e907";
}
.ico-share:before {
  content: "\e908";
}
.ico-star:before {
  content: "\e909";
}
.ico-vip:before {
  content: "\e90a";
}
/* common style */
.page-title{
    font-size:52rpx;
    margin:50rpx 0 80rpx;
    font-weight:700;
}
/*form style*/
.button{
    background-image: linear-gradient(90deg, #FF969D 0%, #FD5C63 100%);
    border-radius:8rpx;
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    color: #FFFFFF;
    letter-spacing: 0;
    padding:26rpx 0;
    min-width:1.4rem;
    line-height:1.4rem;
    transition:.3s;
    text-align:center;
}
.button-hover{
     background-image: linear-gradient(90deg, #FF969D -60%, #FD5C63 150%);
}
.button:after{
    border:none;
}
.mpage{
    margin:0 50rpx;
}
.title-s{
    font-size:40rpx;
}
.page-title text{
    font-size:32rpx;
    color:#4c4c4c;
    font-weight:normal;
}
.abs{
    position:absolute;
    left:0;
    right:0;
    bottom:0;
    top:0;
}
.msg{
    display:none;
    align-items: center;
    justify-content:center;
    z-index:-1;
    transition:.5s;
    animation:fadeOut .3s ease-out;
    position:fixed;
    left:0;
    right:0;
    top:0;
    bottom:0;
}
.msg text{
    padding: 14rpx 28rpx;
    background:rgba(0,0,0,.5);
    color:#fff;
    border-radius:8rpx;
    font-size:24rpx;
    align-self:flex-end;
    margin-bottom:160rpx;
}
.msg.show{
    display:flex;
    
    z-index:100;
    animation:fade .4s ease-in;
}
.bborder{
    position:relative;
}
.border:after{
    content:'';
    position:absolute;
    display:block;
    bottom:0;
    left:0;
    right:0;
    top:0;
    transform-origin:0 0;
    transform:scale(.5);
    border:1px solid #F2F2F2;
    width:200%;
    height:200%;
   
}
.bborder:after{
    content:'';
    position:absolute;
    transform-origin:0 0;
    transform:scale(.5);
    width:200%;
    border:1px solid #F2F2F2;
    bottom:0;
    left:0;
    right:0;
}
@keyframes fade{
    from{
        opacity:0;visibility:hidden;
    }
    to{
        opacity:1;visibility:inherit;
    }
}
@-webkit-keyframes fade{
    from{opacity:0;visibility:hidden;}
    to{opacity:1;visibility:inherit;}
}

@keyframes sliderUp{
    from{
     transform:translateY(100%);visibility:hidden;   
    }
    to{
    transform:translateY(0);visibility:inherit;  
    }
}
@keyframes fadeOut{
    form{opacity:1}
    to{opacity:0;visibility:hidden;}
}

@keyframes sliderDown{
    form{
        transform:translateY(0);visibility:inherit;
    }
    to{
        transform:translateY(100%);visibility:hidden;
    }
}
.fadeOut{
    animation:fadeOut .3s ease-out;
}
.sliderDown{
    animation:sliderDown .4s ease-out;
}

.tabbar{
    position:fixed;
    left:0;
    right:0;
    z-index:98;
    bottom:0;
    display:flex;
    justify-content:space-between;
    align-items:center;
    height:100rpx;
    background:#fff;
}
.tborder:after{
    content:'';
    position:absolute;
    left:0;
    top:0;
    right:0;
    border-top:1px solid #F2F2F2;
    transform:scaleX(0.5);
    transform-origin:0 0;
    width:200%;
}
.tabbar .ico{
    width:25px;
    height:25px;
    background-repeat:no-repeat;
    background-position:0 -25px;
    margin:0 auto;
    background-size:25px;
}
.tabbar text{
    font-size:23rpx;
    color:#7F7F7F;
}
.tabbar navigator{
    display:flex;
    flex-direction:column;
    align-items:center;
    justify-content:center;
    width:25%;
    height:100%;
    text-align:center;
}
.tabbar .active .ico{
    background-position:0 0;
}
.tabbar .active text{
    color:#FD5C63;
}
.empty{
    display:flex;
    align-items:center;
    text-align:center;
    position:absolute;
    top:0;
    bottom:50%;
    left:0;
    right:0;
    justify-content:center;
}
.empty .txt{
    font-size:28rpx;
    color:#999;
    text-align:center;

}
.flex{
    display:flex;
}
.flex-start-between{
    display:flex;
    justify-content:space-between;
    align-items:flex-start;
}
.flex-center-between{
    display:flex;
    justify-content:space-between;
    align-items:center;
}
.flex-center-center{
    display:flex;
    justify-content:center;
    align-items:center;
}

.camp-img image{
    width:100rpx;
    height:100rpx;
    margin-right:30rpx;
}
.camp-star .star{
    display:flex;
    align-items:center;
    height:50rpx;
}
.camp-star .star view{
    display:inline-block;
    font-size:24rpx;
    margin-left:6rpx;
    color:#03A699;
}

.camp-star{
    text-align:right;
}
.price{
    font-size:26rpx;
}
.camp-item{
    margin:0 50rpx;
    border-bottom:1px solid #F2F2F2;
    padding:30rpx 0;
}
.camp-name{
    font-size:36rpx;
    font-weight:800;
    display:flex;
    align-items:center;
}
.camp-name .ico{
    margin-left:10rpx;
    color:#FFDB00;
}
.camp-school{
    font-size:26rpx;
    color:#4c4c4c;
    margin-bottom:26rpx;
}
.camp-tag{
    margin:0;
}
.camp-level{
    font-size:26rpx;
}
.camp-tag view{
   display:inline-block; 
   background:#FABE02;
   color:#fff;
   border-radius:4rpx;
   padding:0 20rpx;
   line-height:50rpx;
   margin-right:10rpx;
   font-size:22rpx;
}
.camp-info{
    margin-top:30rpx;
    font-size:26rpx;
    color:#4C4C4C;
}
.camp-teacher .camp-tag{
    margin:0;
}
.noborder{
    border:none;
}

.footbar{
    display:flex;
    height:145rpx;
    align-items:center;
    justify-content:space-between;
    position:fixed;
    left:0;
    right:0;
    bottom:0;
    z-index:99;
    background:#fff;
}

.footbar .button{
    margin:20rpx 50rpx;
    width:50%;
}

.footbar .tool{
    text-align:center;
    width:40%;
    margin-right:-10%;
}

.footbar .tool view{
     display:inline-block;
     width:40%;
     color:#03A699;
     transition:.4s;
}
.footbar .tool .ico{
    font-size:44rpx;
}
.footbar .tool text{
    font-size:22rpx;
    display:block;
}


.layer{
    position:fixed;
    left:0;
    right:0;
    bottom:0;
    background:#fff;
    z-index:1000;
    padding:50rpx 0;
    visibility:hidden;
}
.layer .page-title{
    margin:0 0 40rpx;
}
.layer.show{
    display:block;
    animation:sliderUp .5s ease-in;
    visibility:inherit;
}

.mask{
    display:none;
    position:fixed;
    z-index:100;
    background:rgba(0,0,0,.5);
    visibility:hidden;
}

.mask.show{
    display:block;
    visibility:inherit;
    animation:fade .2s ease-in;
}
