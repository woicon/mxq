/**app.wxss**/
page,input{
    font-family:PingFang SC,Roboto,sans-serif;
}
@font-face {
	font-family: 'iconfont';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
  font-family: 'iconfont' !important;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-lock:before {
  content: "\e900";
}
.icon-phone:before {
  content: "\e901";
}
/* common style */
.page-title{
    font-size:52rpx;
    margin:50rpx 0 80rpx;
    font-weight:700;
}
/*form style*/
.button{
    background-image: linear-gradient(90deg, #FF969D 0%, #FD5C63 100%);
    border-radius:8rpx;
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    color: #FFFFFF;
    letter-spacing: 0;
    padding:26rpx 0;
    min-width:1.4rem;
    line-height:1.4rem;
    transition:.3s;
    text-align:center;
}
.button-hover{
     background-image: linear-gradient(90deg, #FF969D -60%, #FD5C63 150%);
}
.button:after{
    border:none;
}
.mpage{
    margin:0 50rpx;
}
.title-s{
    font-size:40rpx;
}