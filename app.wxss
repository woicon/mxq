/**app.wxss**/
page,input{
    font-family:PingFang SC,Roboto,sans-serif;
    color:#191919;
}
@font-face {
	font-family: 'fonticon';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: normal;
	font-style: normal;
}
[class^="ico-"], [class*=" ico-"] {
  font-family: 'fonticon' !important;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.ico-add:before {
  content: "\e900";
}
.ico-like:before {
  content: "\e901";
}
.ico-lock:before {
  content: "\e902";
}
.ico-min:before {
  content: "\e903";
}
.ico-ok:before {
  content: "\e904";
}
.ico-phone:before {
  content: "\e905";
}
.ico-search:before {
  content: "\e906";
}
.ico-share:before {
  content: "\e907";
}
.ico-star:before {
  content: "\e908";
}
.ico-vip:before {
  content: "\e909";
}
/* common style */
.page-title{
    font-size:52rpx;
    margin:50rpx 0 80rpx;
    font-weight:700;
}
/*form style*/
.button{
    background-image: linear-gradient(90deg, #FF969D 0%, #FD5C63 100%);
    border-radius:8rpx;
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    color: #FFFFFF;
    letter-spacing: 0;
    padding:26rpx 0;
    min-width:1.4rem;
    line-height:1.4rem;
    transition:.3s;
    text-align:center;
}
.button-hover{
     background-image: linear-gradient(90deg, #FF969D -60%, #FD5C63 150%);
}
.button:after{
    border:none;
}
.mpage{
    margin:0 50rpx;
}
.title-s{
    font-size:40rpx;
}
.abs{
    position:absolute;
    left:0;
    right:0;
    bottom:0;
    top:0;
}
.msg{
    display:none;
    align-items: center;
    justify-content:center;
    z-index:-1;
    transition:.5s;
}
.msg text{
    padding: 14rpx 28rpx;
    background:rgba(0,0,0,.5);
    color:#fff;
    border-radius:8rpx;
    font-size:24rpx;
    align-self:flex-end;
    margin-bottom:160rpx;
}
.msg.show{
    display:flex;
    z-index:100;
}
.bborder{
    position:relative;
}
.bborder:after{
    content:'';
    position:absolute;
    transform-origin:0 0;
    transform:scale(.5);
    width:200%;
    border:1px solid #F2F2F2;
    bottom:0;
    left:0;
    right:0;
}